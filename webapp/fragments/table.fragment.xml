<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout" xmlns:table="sap.ui.table">
	<table:Table id="Table" selectionMode="MultiToggle" visibleRowCount="10" alternateRowColors="true" fixedColumnCount="2" rowActionCount="1"
		  enableGrouping="true" rowSelectionChange="onrow" threshold ="11">
		<table:noData></table:noData>
		<table:toolbar>
			<Toolbar>
				<Title id="Ttitle" text="Cost Association(9)"/>
				<ToolbarSpacer/>
				<Button id="BCopy" icon="sap-icon://copy" text="Copy" visible="false" type="Transparent" press="ongroup"/>
				<Button id="Tfilter" icon="sap-icon://clear-filter" tooltip="Clear all filters" press="clearAllFilters" visible="false"/>
				<Button id="Treset" icon="sap-icon://synchronize" tooltip="Synchronize Data" press="onSync" width="auto" visible="false"/>
				<Button id="Tcreate" icon="sap-icon://create-form" tooltip="Add new rows" press="onAddRows" visible="false"></Button>
			</Toolbar>
		</table:toolbar>
		<table:rowSettingsTemplate>
			<table:RowSettings highlight="{statusIcon}"/>
		</table:rowSettingsTemplate>
		<table:columns>
			<table:Column name="Material" width="7rem" filterProperty="Material" sortProperty="Material" tooltip="Material">
				<Label text="Material" design="Bold" wrapping="true"/>
				<table:template>
					<Input value="{Material}" editable="false"/>
					<!--<Text text="{Material}" wrapping="false"/>-->
				</table:template>
			</table:Column>
			<table:Column name="UPC" width="8rem" sortProperty="UPC" filterProperty="UPC" tooltip="UPC">
				<Label text="UPC" design="Bold" wrapping="true"/>
				<table:template>
					<Input value="{UPC}" editable="false"/>
					<!--<Text text="{LocationCode}" wrapping="false"/>-->
				</table:template>
			</table:Column>
			<!--<table:Column name="Vendor" width="7rem" sortProperty="Vendor" filterProperty="Vendor" tooltip="Vendor">-->
			<!--	<Label text="Vendor"/>-->
			<!--	<table:template>-->
			<!--		<Input value="{Vendor}" editable="false"/>-->
			<!--<Text text="{Vendor}" wrapping="false"/>-->
			<!--	</table:template>-->
			<!--</table:Column>-->
			<table:Column name="Description" width="15rem" sortProperty="Description" filterProperty="Description" tooltip="Description">
				<Label text="Description" design="Bold" wrapping="false"/>
				<table:template>
					<Text text="{Description}" wrapping="false"/>
				</table:template>
			</table:Column>
			<table:Column name="New" width="8rem" tooltip="New" headerSpan="2" hAlign="Center">
				<table:multiLabels>
					<Label text="New" design="Bold"/>
					<Label text="Cost"/>
				</table:multiLabels>
				<table:template>
					<HBox>
						<Input value="{Price}" width="4rem" editable="false" liveChange="selectline"/>
						<Button icon="sap-icon://reset" class="size1 sapUiTinyMarginBegin" press="onreset"/>
					</HBox>
				</table:template>
			</table:Column>
			<table:Column name="valid_from" width="10rem" tooltip="Valid From" hAlign="Center">
				<table:multiLabels>
					<Label text="New" design="Bold"/>
					<Label text="Valid From"/>
				</table:multiLabels>
				<table:template>
					<DatePicker value="{valid_from}" valueFormat="MM/dd/yyyy" editable="false"></DatePicker>
				</table:template>
			</table:Column>
			<!--<table:Column name="valid_from" width="11rem"  tooltip="Valid From">-->
			<!--	<Label text="Valid From"/>-->
			<!--	<table:template>-->
			<!--		<DatePicker value="{valid_from}" valueFormat="MM/dd/yyyy" editable="false"></DatePicker>-->
			<!--	</table:template>-->
			<!--</table:Column>-->
			<!--<table:Column name="valid_to" width="11rem"  tooltip="Valid To">-->
			<!--	<Label text="Valid To" wrapping="true"/>-->
			<!--	<table:template>-->
			<!--		<DatePicker value="{valid_to}" valueFormat="MM/dd/yyyy" editable="false"></DatePicker>-->
			<!--	</table:template>-->
			<!--</table:Column>-->
			<table:Column name="Last_Cost" width="4rem" tooltip="Last Cost">
				<Label text="Case Pack" wrapping="true" design="Bold"/>
				<table:template>
					<Text text="{Last_Cost}" wrapping="false"/>
				</table:template>
			</table:Column>
			<table:Column name="Vendor" width="4.5rem" tooltip="Vendor">
				<Label text="Vendor #" design="Bold" wrapping="true"/>
				<table:template>
					<Text text="{Vendor}" wrapping="false"/>
				</table:template>
			</table:Column>
			
			<table:Column name="Vendor_desc" width="10rem" tooltip="Vendor Name">
				<Label text="Vendor Name" design="Bold" wrapping="true"/>
				<table:template>
					<Text text="{Vendor_desc}" wrapping="false"/>
				</table:template>
			</table:Column>
			<table:Column name="Vendor_item" width="5rem" tooltip="Vendor Item #">
				<Label text="Vendor Item #" wrapping="true" design="Bold" />
				<table:template>
					<Text text="{Vendor_item}" wrapping="false"/>
				</table:template>
			</table:Column>
			<table:Column name="Effective" width="5rem" tooltip="Effective Cost" hAlign="Center">
				<!--<table:multiLabels>-->
				<Label text="Effective Cost" design="Bold" wrapping="true"/>
				<!--	<Label text="Cost"/>-->
				<!--</table:multiLabels>-->
				<table:template>
					<HBox>
						<Label text="{effcost}"/>
						<!--<Input value= width="4rem" editable="false" liveChange="selectline"/>-->
						<!--<Button icon="sap-icon://reset" class="size1 sapUiTinyMarginBegin" press="onreset"/>-->
					</HBox>
				</table:template>
			</table:Column>
			<!--<table:Column name="valid_from" width="6rem" tooltip="Valid From" hAlign="Center">-->
			<!--	<table:multiLabels>-->
			<!--		<Label text="Effective" design="Bold"/>-->
			<!--		<Label text="Valid From"/>-->
			<!--	</table:multiLabels>-->
			<!--	<table:template>-->
			<!--		<Label text="{valid_from}"></Label>-->
			<!--<DatePicker value="{valid_from}" valueFormat="MM/dd/yyyy" editable="false"></DatePicker>-->
			<!--	</table:template>-->
			<!--</table:Column>-->
			<table:Column name="Allowance" width="5.2rem" tooltip="Allowance">
				<Label text="Allowance" wrapping="true" design="Bold"/>
				<table:template>
					<Text text="{Allowance}" wrapping="false"/>
				</table:template>
			</table:Column>
			<table:Column name="Retail" width="5rem" tooltip="Retail" headerSpan="2" hAlign="Center">
				<table:multiLabels>
					<Label text="Retail" design="Bold"/>
					<Label text="Price"/>
				</table:multiLabels>
				<table:template>
					<HBox>
						<Label text="{RetailPrice}"></Label>
						<!--<Input value="" width="4rem" editable="false" liveChange="selectline"/>-->
						<!--<Button icon="sap-icon://reset" class="size1 sapUiTinyMarginBegin" press="onreset"/>-->
					</HBox>
				</table:template>
			</table:Column>
			<table:Column name="gm_p" width="5rem" tooltip="GM %" hAlign="Center">
				<table:multiLabels>
					<Label text="Retail" design="Bold"/>
					<Label text="GM %"/>
				</table:multiLabels>
				<table:template>
					<Label id="gm" text="{gm}"></Label>
					<!--<DatePicker value="{valid_from}" valueFormat="MM/dd/yyyy"></DatePicker>-->
				</table:template>
			</table:Column>
			<!--<table:Column name="valid_from" width="6rem" tooltip="Valid From" hAlign="Center">-->
			<!--	<table:multiLabels>-->
			<!--		<Label text="Retail" design="Bold"/>-->
			<!--		<Label text="Valid From"/>-->
			<!--	</table:multiLabels>-->
			<!--	<table:template>-->
			<!--		<Label text="{valid_from}"></Label>-->
					<!--<DatePicker value="{valid_from}" valueFormat="MM/dd/yyyy"></DatePicker>-->
			<!--	</table:template>-->
			<!--</table:Column>-->
			<table:Column name="gm_wa" width="5rem" tooltip="GM % With Allowance" hAlign="Center">
				<Label text="GM % With Allowance" wrapping="true" design="Bold"/>
				<table:template>
					<Label id="allowgm" text="{allow}"></Label>
					<!--<DatePicker value="{valid_from}" valueFormat="MM/dd/yyyy"></DatePicker>-->
				</table:template>
			</table:Column>
			<!--<table:Column name="valid_to" width="10rem" tooltip="Valid To">-->
			<!--	<Label text="Valid To"/>-->
			<!--	<table:template>-->
			<!--		<DatePicker value="{valid_to}" valueFormat="MM/dd/yyyy"></DatePicker>-->
			<!--	</table:template>-->
			<!--</table:Column>-->
		</table:columns>
		<table:rowActionTemplate>
			<table:RowAction>
				<table:RowActionItem type="Delete" text="Delete" press="onDeletePress"></table:RowActionItem>
			</table:RowAction>
		</table:rowActionTemplate>
	</table:Table>
</core:FragmentDefinition>